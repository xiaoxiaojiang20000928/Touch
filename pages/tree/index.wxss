.container {
  height: 100vh;
  background-image: url("https://www.yymlcy.cn/imgs/spring.png");
  background-size: cover;
}
.container_postion {
  width: 100rpx;
  height: 100rpx;
  position: relative;
}
.container_postion_1 {
  width: 150rpx;
  position: absolute;
  left: 500rpx;
  top: 960%;
  z-index: 1;
}
.container_postion_2 {
  width: 60rpx;
  position: absolute;
  left: 20rpx;
  top: 30rpx;
}
.contentcontainer {
  font-family: 'haibao';
  margin: 60rpx auto 0 auto;
  width: 92%;
  height: 1030rpx;
  padding: 60rpx 0 60rpx 0;
  background-color: #F3FFE7;
  overflow: auto;
  color: #513530;
}
.contentcontainer_one {
  display: flex;
  margin-top: 8rpx;
  align-items: center;
}
.contentcontainer_one_photo {
  width: 120rpx;
  padding-left: 20rpx;
}
.contentcontainer_one_text {
  padding-left: 80rpx;
  flex: 1;
}
.contentcontainer_one_delete {
  width: 50rpx;
  padding-right: 10rpx;
}
.contentcontainer_item {
  padding: 0 20rpx 0 20rpx;
  display: flex;
  justify-content: space-around;
  flex-direction: column;
  margin: 30rpx 0 30rpx 0;
  height: 120rpx;
  background-color: #F3FFE7;
  border-radius: 20rpx;
}
.contentcontainer_item_time {
  font-size: 28rpx;
  font-weight: bold;
}
.contentcontainer2 {
  font-family: 'haibao';
  margin: -10rpx auto 0 auto;
  display: flex;
  height: 1178rpx;
  width: 660rpx;
  flex-direction: column;
  align-items: center;
  background-image: url("https://www.yymlcy.cn/imgs/tree_background.png");
  background-size: 100%;
  padding: 20rpx;
  background-repeat: no-repeat;
  color: #513530;
}
.contentcontainer2 > view {
  display: flex;
  margin-top: 100rpx;
  padding-left: 30rpx;
  width: 100%;
  height: 350rpx;
}
.contentcontainer2_item1 {
  font-size: 40rpx;
  font-weight: bold;
  flex: 1;
  margin-top: -50rpx;
}
.contentcontainer2_item2 {
  flex: 4;
  display: flex;
  flex-wrap: wrap;
  margin-top: 20rpx;
}
.contentcontainer2_item2 image {
  width: 120rpx;
  margin: 10rpx;
}
.contentcontainer2_item2 .cover {
  border: 2rpx solid gray;
}
.contentcontainer2_item3 {
  background-image: url("https://www.yymlcy.cn/imgs/treetalk.png");
  flex: 4;
  margin-top: 20rpx;
  margin-right: 18rpx;
  padding: 10rpx;
}
.contentcontainer2 .add {
  margin-top: 120rpx;
}
