.container {
  font-family: 'haibao';
  background-image: url("http://1.116.141.149/imgs/mine_background.png");
  background-size: cover;
  height: 100vh;
}
.userinfocontainer {
  height: 200rpx;
  display: flex;
  align-items: center;
}
.userinfocontainer image {
  margin-left: 43rpx;
  width: 140rpx;
  border-radius: 50%;
  border: 4px solid white;
}
.userinfocontainer text {
  padding-left: 40rpx;
  letter-spacing: 5rpx;
  font-size: 45rpx;
  font-weight: bold;
  color: #513530;
}
.contentcontainer {
  margin: 0 auto;
  width: 92%;
  height: 60vh;
  background-color: white;
  border-radius: 6px;
}
.contentcontainer_item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 40rpx;
}
.contentcontainer_item view {
  display: flex;
  align-items: center;
}
.contentcontainer_item view text {
  font-size: 32rpx;
  padding-left: 20rpx;
}
.contentcontainer_item view image {
  width: 40rpx;
}
.contentcontainer_item image {
  width: 30rpx;
}
button {
  width: 60%;
  font-size: 26rpx;
  margin-top: 50rpx;
  background-color: #fdc390;
}
